#include "../inc/Matrix.h"

vectorType aInst, bInst, pInst;

void setInputMatrices(VectorArray A, VectorArray B)
{
	memcpy(&aInst, &A, sizeof(aInst));
	memcpy(&bInst, &B, sizeof(bInst));
}

void displayMatrix(VectorArray input)
{
	unsigned char i = 0;
	for(; i < MSIZE*MSIZE; i++)
	{
		printf(input[i/MSIZE].comp[i%MSIZE]);
		if (i+1 % MSIZE == 0)
		{
			print("\n");
		}
	}
}


